{
  "expo": {
    "name": "AwesomeProject",
    "slug": "AwesomeProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/IconoCoyi/Icon_1024x1024.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cdc.appcdc"
    },
    "android": {
      "package": "com.cdc.appcdc",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/IconoCoyi/Icon_29x29@2x.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/IconoCoyi/Icon_20x20@2x.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/IconoCoyi/Icon_20x20@2x.png",
          "color": "#6fff00"
        }
      ]
    ]
  }
}
